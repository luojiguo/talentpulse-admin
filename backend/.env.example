# 数据库配置 - Supabase
DATABASE_URL=postgresql://postgres:password@host:5432/postgres
# Supabase 项目配置
SUPABASE_PROJECT_ID=your-project-id
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_PUBLISHABLE_KEY=your-publishable-key

# 服务器配置
PORT=8001
NODE_ENV=development

# JWT配置
JWT_SECRET=your-secret-key
JWT_EXPIRES_IN=7d

# CORS配置
CORS_ORIGINS=http://localhost:3000,http://localhost:3002

# 日志配置
LOG_LEVEL=info

# 邮件服务配置 (LuckyColaAI)
LUCKYCOLA_COLAKEY=your-colakey
LUCKYCOLA_SMTP_EMAIL=your-email@example.com
LUCKYCOLA_SMTP_CODE=your-smtp-code
LUCKYCOLA_SMTP_CODE_TYPE=qq # 可选值: qq, 163, 126

# ===========================
# AI 服务配置
# ===========================
QIANWEN_API_KEY=sk-your-api-key
QIANWEN_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
